{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch csv-to-toml",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rs/target/debug/csv-to-toml.exe",
            "args": ["${workspaceFolder}/../../setup/data/csv/1S.csv", "${workspaceFolder}/../../setup/data/toml/"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": []
            //"console": "externalTerminal"
        },
        {
            "name": "(Windows) Launch toml-to-md",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rs/target/debug/toml-to-md.exe",
            "args": ["${workspaceFolder}/../../setup/data/toml/2H.toml", "${workspaceFolder}/../../setup/data/md/"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": []
            //"console": "externalTerminal"
        }

    ]
}